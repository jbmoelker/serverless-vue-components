{
  "name": "serverless-vue-components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "run-p build:* --print-label",
    "build:js": "vue-cli-service build --formats commonjs --target lib ./src/components/HelloWorld.vue --filename HelloWorld",
    "postbuild": "node scripts/css-to-js.js",
    "dev": "netlify dev",
    "dev:app": "vue-cli-service serve",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vuedoc/parser": "^3.0.0",
    "core-js": "^3.6.5",
    "netlify-cli": "^2.68.0",
    "vue": "^2.6.11",
    "vue-server-renderer": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
